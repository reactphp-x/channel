{
    "name": "reactphp-x/channel",
    "description": "A Channel implementation for ReactPHP, similar to Swoole's Coroutine\\Channel",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "react/async": "^4.0",
        "react/event-loop": "^1.4",
        "react/promise": "^3.0",
        "react/promise-timer": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "react/http": "^1.11"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ReactphpX\\Channel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReactphpX\\Channel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "wpjscc",
            "email": "wpjscc@gmail.com"
        }
    ]
}
